              <my:Switch Height="40" x:Name="BtnOut40" VariableName="P40" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut41" VariableName="P41" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut42" VariableName="P42" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut43" VariableName="P43" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut44" VariableName="P44" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut45" VariableName="P45" Width="40"/>
              <my:Switch Height="40" x:Name="BtnOut46" VariableName="P46" Width="40"/>
