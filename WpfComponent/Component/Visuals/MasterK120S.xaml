              <my:CircularLight Height="40" LightOn="False" x:Name="In2" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In3" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In4" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In5" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In6" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In7" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In8" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In9" Width="40"/>
              <my:CircularLight Height="40" LightOn="False" x:Name="In10" Width="40"/>    
              <my:CircularLight Height="40" LightOn="False" x:Name="In11" Width="40"/>    
            </StackPanel>  
            <StackPanel Grid.Row="0" Name="stackPanelINText" Orientation="Horizontal">
              <TextBlock Height="20" Width="40" Padding="17,1" Text="00"/>
