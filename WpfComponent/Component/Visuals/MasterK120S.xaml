              <TextBlock Height="20" Width="40" Padding="17,1" Text="02"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="03"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="04"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="05"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="06"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="07"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="08"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="09"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="0A"/>
              <TextBlock Height="20" Width="40" Padding="17,1" Text="0B"/>                
            </StackPanel> 
            <StackPanel Grid.Row="0" Name="stackPanelInButton" Orientation="Horizontal">
              <my:Switch Height="40" x:Name="BtnIn0" VariableName="P00" Width="40"/>
