              <my:Switch Height="40" x:Name="BtnIn7" VariableName="P07" Width="40"/>
              <my:Switch Height="40" x:Name="BtnIn8" VariableName="P08" Width="40"/>
              <my:Switch Height="40" x:Name="BtnIn9" VariableName="P09" Width="40"/>
              <my:Switch Height="40" x:Name="BtnIn10" VariableName="P10" Width="40"/>
              <my:Switch Height="40" x:Name="BtnIn11" VariableName="P11" Width="40"/>
            </StackPanel> 
        </StackPanel>
        <StackPanel Grid.Row="1" Name="stackPanel2">
            <TextBlock HorizontalAlignment="Left" Padding="12,1" Height="21" Name="textBlock2" Width="120" Text="OUT" />
