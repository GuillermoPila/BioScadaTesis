              <my:CircularLight Height="40" LightOn="False" x:Name="Out47" Width="40"/>              
            </StackPanel> 
            <StackPanel Grid.Row="0" Name="stackPanelOutText" Orientation="Horizontal">
              <TextBlock Height="20" Width="40" Padding="17,1" Text="40"/>
